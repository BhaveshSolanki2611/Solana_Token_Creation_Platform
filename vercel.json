{
  "version": 2,
  "name": "solana-token-creation-platform",
  "buildCommand": "npm run vercel-build",
  "outputDirectory": "client/build",
  "installCommand": "npm run install-all",
  "functions": {
    "api/index.js": {
      "maxDuration": 30,
      "memory": 1024
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.js"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "regions": ["iad1"]
}